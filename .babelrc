{
  "presets": [
    "mobx",
    "react-app",
    "@babel/preset-typescript",
    "@babel/preset-env",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ],
  "assumptions": {
    "setPublicClassFields": false,
    "privateFieldsAsProperties": true
  }
}
